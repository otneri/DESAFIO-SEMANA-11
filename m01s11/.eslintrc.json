{
    "settings": {
        "react": {
          "version": "detect"
        }
      },

    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard",
        "plugin:react-hooks/recommended",
        "plugin:react/jsx-runtime",
        "plugin:jsx-a11y/recommended",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react", "jsx-a11y"],
    "rules": {
        "prefer-const": "warn",
        "no-unused-vars": "off"
    }
}
